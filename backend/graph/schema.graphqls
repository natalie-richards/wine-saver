type Bookmark {
  name: String
  grape: String
  region: String
  location: String
}

input AddBookmarkRequest {
  name: String
  grape: String
  region: String
  location: String
}

type User {
  id: ID!
  name: String!
}

type Query {
  listBookmarks: [Bookmark]
}

type Mutation {
  addBookmark(input: AddBookmarkRequest!): Bookmark!
}
