type Bookmark {
  name: String
  grape: String
  region: String
  location: String
  notes: String
}

input AddBookmarkRequest {
  name: String
  grape: String
  region: String
  location: String
  notes: String
}

type User {
  id: ID!
  first_name: String
  last_name: String
  email: String
}

type Query {
  listBookmarks: [Bookmark]
}

type Mutation {
  addBookmark(input: AddBookmarkRequest!): Bookmark!
}
